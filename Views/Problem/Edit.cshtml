@model MyWebApp.ViewModels.EditProblemViewModel

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edit</h2>

<form action="/Problem/UploadFile" method="post" enctype="multipart/form-data" id="imageform"></form>

@using (Html.BeginForm("Edit"))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        @Html.Partial("_CreateEdit", Model)

        <div class="form-group">
        @Html.LabelFor(model => model.IsBlocked, htmlAttributes: new { @class = "control-label col-md-2" })

        <div class="col-md-10">
            @Html.CheckBoxFor(model => model.IsBlocked, new { @class = "form-control" })
        </div>
    </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input id="edit" type="submit" value="Edit" class="btn btn-default " />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>