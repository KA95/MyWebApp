@model IEnumerable<MyWebApp.Models.ApplicationUser>
@{
    ViewBag.Title = "Home Page";
}
@Html.Partial("_NavMenu")
<div class="jumbotron">

    <table title="Users" class="table table-striped table-hover ">
        <thead>
            <tr>
                <th>User</th>
                @*<th>Solved</th>*@
            </tr>
        </thead>
        @foreach (var user in Model)
        {
            <tr>
                <th>
                    @Html.ActionLink(user.UserName, "Show", "User", new { id = user.Id }, null)
                </th>
                @*<th>
                    @user.SolvedProblems.Count
                </th>*@
                <th>
                    @Html.ActionLink("Delete", "DeleteUser", "Admin", new { id = user.Id }, null)
                </th>
                <th>
                    @Html.ActionLink("Edit", "Edit", "User", new { id = user.Id }, null)
                </th>
            </tr>
        }
    </table>
</div>
